{
    "version": 2,
    "name": "server",
    "public": true,
    "builds": [
      { "src": "Server.js", "use": "@vercel/node" },
      { "src": "dist/**", "use": "@vercel/static" }
    ],
    "routes": [
      {
        "src": "^/api/(.*)",
        "dest": "/Server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
      }
    ]
  }
  