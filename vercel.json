{
    "version": 2,
    "name": "server",
    "public": true,
    "builds": [
      { "src": "Server.js", "use": "@vercel/node" },
      { "src": "public/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      {
        "src": "^/api/(.*)", 
        "dest": "/"
      },
      {
        "src": "/(.*)", 
        "dest": "/Server.js"
      }
    ]
  }
  